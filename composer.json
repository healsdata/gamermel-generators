{
    "name": "healsdata/gamermel-generators",
    "license": "MIT",
    "type": "project",
    "require": {
        "slim/slim": "^4.14",
        "slim/psr7": "^1.7",
        "slim/twig-view": "^3.4",
        "zeuxisoo/slim-whoops": "^0.7.3",
        "google/apiclient": "^2.18",
        "php-di/slim-bridge": "^3.4"
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Healsdata\\GamermelGenerators\\": "src/"
        }
    },
    "config": {
        "process-timeout": 600
    },
    "scripts": {
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "extra": {
        "google/apiclient-services": [
            "Sheets"
        ]
    }
}
